{
  "name": "Rapid Note",
  "description": "Save your notes effectively.",
  "background": { "scripts": ["background/index.js"] },
  "content_scripts": [
    {
      "js": ["content/index.js"],
      "matches": ["https://*/*", "http://*/*"]
    }
  ],
  "browser_action": { 
    "default_popup": "pages/popup/index.html",
    "default_icon": {
      "16": "/images/demo.png",
      "32": "/images/demo.png",
      "48": "/images/demo.png",
      "128": "/images/demo.png"
    }
  },
  "icons": {
    "16": "/images/demo.png",
    "32": "/images/demo.png",
    "48": "/images/demo.png",
    "128": "/images/demo.png"
  },
  "content_security_policy": "script-src 'self' https://apis.google.com/js/api.js; https://apis.google.com/_/scs/abc-static/_/js/k=gapi.lb.en.iTmf4rxOyWc.O/m=gapi_iframes/rt=j/sv=1/d=1/ed=1/rs=AHpOoo-LTnDn-AS2QlMWYZdnaV1OuFR7Iw/cb=gapi.loaded_0?le=scs; object-src 'self'"
}
